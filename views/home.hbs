<h1>Male table</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Apellido</th>
      <th scope="col">Gender</th>
      <th scope="col">Timestamp</th>
    </tr>
  </thead>
  <tbody>
    {{#each male}}
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.name}}</td>
      <td>{{this.apellido}}</td>
      <td>{{this.gender}}</td>
      <td>{{this.timestamp}}</td>
    </tr>
    
    {{/each}}

  </tbody>
</table>
